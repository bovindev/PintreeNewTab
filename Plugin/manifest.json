{
  "manifest_version": 3,
  "name": "PintreeNewTab",
  "version": "1",
  "description": " 管理书签",
  "permissions": [
    "bookmarks"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "书签"
  },
  "icons": {
    "16": "assets/favicon/IMG_202409239328_16x16.png",
    "48": "assets/favicon/IMG_202409234619_48x48.png",
    "128": "assets/favicon/IMG_202409238296_128x128.png"
  }
}